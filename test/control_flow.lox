// Nested for loops with break and continue
var sum = 0;
for (var i = 0; i < 3; i = i + 1) {
  for (var j = 0; j < 3; j = j + 1) {
    if (i == 1 and j == 1) {
      continue;
    } else if (i == 2 and j == 2) {
      break;
    }
    sum = sum + i * j;
    print sum;
  }
}

//// While loop with break and continue
var k = 0;
while (k < 10) {
  if ((k + 1) / 2 - k / 2 == 0) {
    k = k + 1;
    continue;
  } else if (k > 7) {
    break;
  }
  print k;
  k = k + 1;
}

// If-ElseIf-Else control flow test
if (sum > 5) {
  print "Sum is greater than 5";
} else if (sum == 5) {
  print "Sum equals 5";
} else {
  print "Sum is less than 5";
}